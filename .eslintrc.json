{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "mocha": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "no-console": "off",
      "no-restricted-syntax": "off",
      "no-prototype-builtins": "off"
    }
  }
  